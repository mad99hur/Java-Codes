import java.util.*;
import java.lang.*;
import java.io.*;

class LargestElement
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        int[] arr = {3,4,7,69,5,2};
        int largest=0;
        largest = arr[0];
        for(int i=0; i<=arr.length-1; i++){
            if(arr[i]>= largest){
                
                largest = arr[i];
            }
        }
        System.out.println("largest element is: "+largest);
	}
}
